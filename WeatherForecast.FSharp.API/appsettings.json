{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtOptions": {
    "Key": "AuthenticationKey",
    "Issuer": "ParcelTracker_API",
    "Audience": "ParcelTracker_Client",
    "Lifetime": 30
  },
  "ExpirationTime": 20,
  "IconUrlFormat": "http://openweathermap.org/img/wn/%s@2x.png",
  "APIUrlFormat": "http://api.openweathermap.org/data/2.5/forecast?q=%s&units=metric&appid=%s"
}
